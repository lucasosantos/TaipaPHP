{
    "name": "lucasosantos/taipaphp",
    "description": "Framework php simples e rapido",
    "type": "project",
    "keywords": ["framework", "taipa"],
    "license": "MIT",
    "autoload": {
        "files": [
            "taipa-config.php",
            "taipa-methods.php",
            "app/router/router.php",
            "app/router/web.php",
            "app/router/api.php",
            "app/core/Controller.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require": {
        "php": ">=8.1",
        "vlucas/phpdotenv": "^5.5",
        "firebase/php-jwt": "^6.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
